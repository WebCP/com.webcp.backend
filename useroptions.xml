<?xml version="1.0"?>
<!DOCTYPE data SYSTEM "http://www.woltlab.com/DTD/options.dtd">
<data>
	<import>
		<categories>
			<category name="account.disk">
				<parent>account</parent>
			</category>
		</categories>

		<options>
			<option name="diskspace">
				<categoryname>account.disk</categoryname>
				<optiontype>integer</optiontype>
				<visible>0</visible>
